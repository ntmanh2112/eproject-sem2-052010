		<![CDATA[SELECT
     TBL_LEAVEAPP."ID_USER",
     TBL_USER."USERNAME",
     TBL_LEAVEAPP."DATEFROM",
     TBL_LEAVEAPP."DATETO",
     TBL_LEAVEAPP."REASON"
FROM
      "TBL_USER"TBL_USER INNER JOIN "TBL_LEAVEAPP" TBL_LEAVEAPP ON TBL_USER."ID_USER" = TBL_LEAVEAPP."ID_USER"
WHERE
     TBL_LEAVEAPP.STATUSLEAVE = 'FINISH'
 AND TBL_USER.ID_POSITION = '2'
 AND DATEPART(MONTH,TBL_LEAVEAPP.DATEFROM) = DATEPART(MONTH,GETDATE())
 AND DATEPART(YEAR,TBL_LEAVEAPP.DATEFROM) = DATEPART(YEAR,GETDATE())]]>
